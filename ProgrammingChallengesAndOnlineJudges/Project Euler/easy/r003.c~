/*
	The prime factors of 13195 are 5, 7, 13 and 29.

	What is the largest prime factor of the number 600851475143 ?
*/

#include<stdio.h>
#include<math.h>
void main()
{
 long i, j, lpf;
 boolean b;
 i = 1;
 while(i < (600851475143 / 2))
 {
  i++;
  if((600851475143 % i) == 0)
  {
   j = 1;
   b = true;
   while(j < ((long) sqrt(i)))
   {
    if((i % j) == 0)
    {
     b = false;
    }
    j++;
   }
   if(b)
   {
    lpf = i;
   }
  }
 }
 printf("\n\tLargest prime factor of 600851475143 is %ld\n\n", lpf);
}
